
<%- include('../partials/header') %>


<h1>Edit Prayer</h1>
        <form action="/prayers<%= Prayer.id %>?_method=PUT" method="POST">
          <input type="text" value="<%= Prayer.Prayer%>" name="prayer">
          <input type="checkbox" name="done" <%= Prayer.done ? 'checked' : ''%>>
          <button type="submit">Save Prayer</button>
        </form>




<%- include('../partials/footer') %>
