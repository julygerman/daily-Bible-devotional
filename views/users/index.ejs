<%- include('../partials/header') %>

<body>
 
  <h3>User List Page</h3>
<% users.forEach(u => { %>
  <a href="/users/<%= u._id %>"> 
    <div class="card" style="width: 1000px; padding: 20px; background-color: seashell;">
      <div class="card-body">
      <img height="75" id="avatarPhoto" src="<%= u.avatar ? u.avatar : 'http://theoldreader.com/kittens/300/300/' %>" alt=""> 
        <%= u.name %></a> - Joined on <%= u.createdAt.toLocaleString().split(',')[0] %>
      </div>
    </div><br>
<% }) %>


<%- include('../partials/footer') %>